{
  "store.custom#faq": {
    "children": [
      "flex-layout.row#faq",
      "back-to-top-button"
    ],
    "props":{"blockClass":"faq-container"}
  },

  "flex-layout.row#faq": {
    "props": {
      "blockClass": "faq-row"
    },
    "children": [
      "flex-layout.col#faq"
    ]
  },

  "flex-layout.col#faq": {
    "children": [
      "rich-text#faq",
      "disclosure-layout-group#group"
    ]
  },
  "rich-text#faq": {
    "props": {
      "text": "Preguntas frecuentes",
      "blockClass":"faq-title"
    }
  },

  "disclosure-layout-group#group": {
    "children": [
      "disclosure-layout#first",
      "disclosure-layout#second",
      "disclosure-layout#third",
      "disclosure-layout#fourth",
      "disclosure-layout#5",
      "disclosure-layout#6",
      "disclosure-layout#7",
      "disclosure-layout#8",
      "disclosure-layout#9",
      "disclosure-layout#10",
      "disclosure-layout#11",
