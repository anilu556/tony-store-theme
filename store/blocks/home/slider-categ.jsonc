  // Carousel Categorias ###########################################################################################################################################
  "rich-text#slider-title-categorias": {
    "props": {
      "text": "Todo para lorem ipsum",
      "blockClass": "shelfTitle-cats"
    }
  },
  "slider-layout#categorias": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 5,
        "phone": 3
      },
      "infinite": true,
      "showNavigationArrows": "never",
      "showPaginationDots": "always",
      "blockClass": "carousel-categories",
      "autoplay":{
        "timeout":3000,
        "stopOnHover":true
      }
    },
    "children": [
      "flex-layout.row#category-1",
      "flex-layout.row#category-2",
      "flex-layout.row#category-3",
      "flex-layout.row#category-4",
      "flex-layout.row#category-5",
