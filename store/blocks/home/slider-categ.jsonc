      "flex-layout.row#category-7",
      "flex-layout.row#category-8",
      "flex-layout.row#category-9"

    ]
  },
  "flex-layout.row#category-1":{
    "children":[
      "image#category-1",
      "rich-text#category-1"
    ],
    "props":{
      "blockClass": "category-container"
    }
  },
  "image#category-1": {
    "props": {
      "title": "imagen cat 1",
      "src": "/arquivos/categ-dummy.png",
      "maxHeight": 155,
      "alt": "category Image",
      "link": {"url": "/#"}
